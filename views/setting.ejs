<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <form>
        帳號<input type="text" name="username" id="username">
        <button type="submit">GO</button>
    </form>
    <%- include('wedgets/cdn') %>
    <%- include('wedgets/utils') %>
    <script>
        $('form').addEventListener('submit', async (e) => {
            e.preventDefault()
            let newUserInfo = {}
            for(ipt of e.target){
                if(ipt.type === 'text'){
                    newUserInfo[ipt.name] = ipt.value
                }
            }
            let {
                data
            } = await axios.patch('/api/user/', newUserInfo)
            console.log(data)
        })
    </script>
</body>
</html>